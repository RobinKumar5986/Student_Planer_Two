<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StudentPlanerTwo" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FFDD00</item>
        <item name="colorPrimaryVariant">#FFDD00</item>
        <item name="colorOnPrimary">#F5A417</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFDD00</item>
        <item name="colorSecondaryVariant">#F57F17</item>
        <item name="colorOnSecondary">#4B4201</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.StudentPlanerTwo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.StudentPlanerTwo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.StudentPlanerTwo.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>